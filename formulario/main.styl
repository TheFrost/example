body
  text-align center

header,
section,
footer,
aside,
nav,
article,
figure,
figcaption,
hgroup
  display block

#reproductor
  width 720px
  margin 20px auto
  padding 5px
  background-color #999
  border-radius 5px

nav
  margin 5px 0

#botones
  float left
  width 100px
  height 20px
  transition all 0.25s

#barra
  position relative
  float left
  width 600px
  height 16px
  padding 2px
  border 1px solid #ccc
  background-color #eee

#progreso
  position absolute
  width 0
  height 16px
  background-color rgba(0, 0, 150, .2)
